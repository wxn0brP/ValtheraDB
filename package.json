{
	"name": "@wxn0brp/db",
	"version": "0.0.6",
	"main": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"description": "A simple file-based database management system with support for CRUD operations, custom queries, and graph structures.",
	"homepage": "https://github.com/wxn0brP/database",
	"repository": {
		"type": "git",
		"url": "https://github.com/wxn0brP/database.git"
	},
	"keywords": [
		"database",
		"file-based",
		"CRUD",
		"graph",
		"query"
	],
	"author": "wxn0brP",
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"got": "^14.4.2",
		"json5": "^2.2.3",
		"readline": "^1.3.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.5",
		"@types/got": "^9.6.12",
		"@types/node": "^22.10.2",
		"ava": "^6.2.0",
		"body-parser": "^1.20.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"jwt-simple": "^0.5.6",
		"node-cache": "^5.1.2",
		"typescript": "^5.7.2"
	},
	"scripts": {
		"build:esm": "tsc",
		"build:cjs": "tsc --module CommonJS --outDir ./dist/cjs --declarationDir ./dist/cjs",
		"build": "npm run build:esm && npm run build:cjs",
		"postbuild": "node scripts/postbuild.js",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	}
