{
	"name": "@wxn0brp/db",
	"version": "0.1.1",
	"main": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"description": "A simple file-based database management system with support for CRUD operations, custom queries, and graph structures.",
	"homepage": "https://github.com/wxn0brP/ValtheraDB",
	"repository": {
		"type": "git",
		"url": "https://github.com/wxn0brP/ValtheraDB.git"
	},
	"keywords": [
		"database",
		"file-based",
		"CRUD",
		"graph",
		"query"
	],
	"author": "wxn0brP",
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"json5": "^2.2.3",
		"ky": "^1.7.4",
		"readline": "^1.3.0"
	},
	"devDependencies": {
		"@types/node": "^22.10.2",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.7.2"
	},
	"scripts": {
		"build:esm": "tsc && tsc-alias -p tsconfig.json",
		"build:cjs": "tsc -p tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
		"build": "npm run build:esm && npm run build:cjs",
		"prepare": "npm run build",
		"postversion": "git push && git push --tags"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/esm/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		},
		"./*": {
			"types": "./dist/esm/*",
			"import": "./dist/esm/*",
			"require": "./dist/cjs/*"
		}
	}
}
